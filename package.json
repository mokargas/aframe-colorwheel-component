{
  "name": "aframe-colorwheel-component",
  "version": "1.1.0",
  "description": "A-Frame Color Wheel component, designed for A-Frame Material",
  "main": "index.js",
  "unpkg": "dist/aframe-colorwheel-component.min.js",
  "scripts": {
    "dev": "NODE_ENV=dev webpack-dev-server --progress --colors --inline --hot -d --host 0.0.0.0",
    "dist": "webpack index.js dist/aframe-colorwheel-component.js && webpack -p index.js dist/aframe-colorwheel-component.min.js",
    "lint": "semistandard -v | snazzy",
    "prepublish": "npm run dist",
    "ghpages": "ghpages",
    "start": "npm run dev",
    "test": "karma start ./tests/karma.conf.js",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mokargas/aframe-colorwheel-component.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "colorwheel"
  ],
  "author": "Mo Kargas (DEVLAD) <mo@devlad.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mokargas/aframe-colorwheel-component/issues"
  },
  "homepage": "https://github.com/mokargas/aframe-colorwheel-component#readme",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-preset-es2015": "^6.16.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.2",
    "chai": "^3.4.1",
    "chai-shallow-deep-equal": "^1.3.0",
    "copy-to-clipboard": "^3.0.8",
    "ghpages": "^0.0.8",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "mocha": "^2.3.4",
    "randomcolor": "^0.4.4",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "snazzy": "^4.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.15.1"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  }
}
